# 该文件包含fish通用变量定义。
# 版本: 3.0

# 语言设置
SETUVAR --export LANG:zh_CN\x2eUTF\x2d8
SETUVAR --export LANGUAGE:zh_CN\x3aen_US
SETUVAR --export LC_CTYPE:en_US\x2eUTF\x2d8

# 设定输入法
SETUVAR --export GTK_IM_MODULE:fcitx
SETUVAR --export QT_IM_MODULE:fcitx
SETUVAR --export XMODIFIERS:\x40im\x3dfcitx
SETUVAR --export SDL_IM_MODULE:fcitx

# nnn 书签，选择，插件，缓存
SETUVAR --export NNN_BMS:a\x3a\x7e/a\x3bx\x3a\x7e/xz\x3bj\x3a\x7e\x3bg\x3a\x7e/gz
SETUVAR --export NNN_SEL:/tmp/\x2esel
SETUVAR --export NNN_PLUG:
SETUVAR --export NNN_FIFO:/tmp/nnn\x2efifo

# 默认编辑器
SETUVAR --export EDITOR:nvim

# 控制键替换大写锁定键
SETUVAR --export XKB_DEFAULT_OPTIONS:ctrl\x3anocaps


# fish 自带变量
SETUVAR __fish_initialized:3100
SETUVAR fish_color_autosuggestion:555\x1ebrblack
SETUVAR fish_color_cancel:\x2dr
SETUVAR fish_color_command:005fd7
SETUVAR fish_color_comment:990000
SETUVAR fish_color_cwd:green
SETUVAR fish_color_cwd_root:red
SETUVAR fish_color_end:009900
SETUVAR fish_color_error:ff0000
SETUVAR fish_color_escape:00a6b2
SETUVAR fish_color_history_current:\x2d\x2dbold
SETUVAR fish_color_host:normal
SETUVAR fish_color_host_remote:yellow
SETUVAR fish_color_match:\x2d\x2dbackground\x3dbrblue
SETUVAR fish_color_normal:normal
SETUVAR fish_color_operator:00a6b2
SETUVAR fish_color_param:00afff
SETUVAR fish_color_quote:999900
SETUVAR fish_color_redirection:00afff
SETUVAR fish_color_search_match:bryellow\x1e\x2d\x2dbackground\x3dbrblack
SETUVAR fish_color_selection:white\x1e\x2d\x2dbold\x1e\x2d\x2dbackground\x3dbrblack
SETUVAR fish_color_status:red
SETUVAR fish_color_user:brgreen
SETUVAR fish_color_valid_path:\x2d\x2dunderline
SETUVAR fish_greeting:
SETUVAR fish_key_bindings:fish_default_key_bindings
SETUVAR fish_pager_color_completion:\x1d
SETUVAR fish_pager_color_description:B3A06D\x1eyellow
SETUVAR fish_pager_color_prefix:white\x1e\x2d\x2dbold\x1e\x2d\x2dunderline
SETUVAR fish_pager_color_progress:brwhite\x1e\x2d\x2dbackground\x3dcyan
